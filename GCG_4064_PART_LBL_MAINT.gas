		F.Intrinsic.Math.Round(V.Local.iRet,4,V.Local.iRet)
		F.Intrinsic.String.ConvertToString(V.Local.iRet,V.Local.sRet)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.String.IsInString(V.Local.sRet.Trim,".",True,V.Local.bExists)
	F.Intrinsic.Control.If(V.Local.bExists,=,True)
		F.Intrinsic.String.Split(V.Local.sRet.Trim,".",V.Local.sRet)
		F.Intrinsic.String.RPad(V.Local.sRet(1).Trim,"0",4,V.Local.sret2)
		F.Intrinsic.Control.If(V.Local.sRet(0),=,"0")
			F.Intrinsic.String.Concat(".",V.Local.sret2,V.Local.sRet)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sRet(0).Trim,".",V.Local.sRet2.Trim,V.Local.sRet)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Variable.AddRV("Result",V.Local.sRet)
F.Intrinsic.Control.EndIf

