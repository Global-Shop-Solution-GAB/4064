V.Local.iDenom.Declare(Float)
V.Local.iRet.Declare(Float)
V.Local.sRet.Declare(String)
V.Local.sret2.Declare(String)
V.Local.bExists.Declare(Boolean)


V.Local.sRet.Set(V.Args.ret)
F.Intrinsic.Control.If(V.Local.sRet,<>,"---")

	F.Intrinsic.String.IsInString(V.Local.sRet.Trim,"/",True,V.Local.bExists)
	F.Intrinsic.Control.If(V.Local.bExists,=,True)
		F.Intrinsic.String.Split(V.Local.sRet,"/",V.Local.sRet)
		F.Intrinsic.Math.ConvertToFloat(V.Local.sRet(0).Trim,V.Local.iNum)
		F.Intrinsic.Math.ConvertToFloat(V.Local.sRet(1).Trim,V.Local.iDenom)
